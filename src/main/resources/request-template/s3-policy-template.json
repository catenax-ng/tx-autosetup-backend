{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Deny",
			"Action": [
				"s3:CreateBucket",
				"s3:DeleteBucket",
				"s3:ForceDeleteBucket"
			],
			"Resource": [
				"arn:aws:s3:::${bucket}/*",
				"arn:aws:s3:::${bucket}*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:Put*",
				"s3:DeleteObject",
				"s3:DeleteObject*",
				"s3:Get*",
				"s3:List*"
			],
			"Resource": [
				"arn:aws:s3:::${bucket}*",
				"arn:aws:s3:::${bucket}/*"
			]
		}
	]
}